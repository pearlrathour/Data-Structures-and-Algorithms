//string palindrome function
bool palin(string s){ 
    int i=0; int j=s.le()-1; 
    while(i<=j){ 
        if(s[i]!=s[j]) return false;
        i++; j--;
    } 
    return true; 
}

//string to integer conversion function
int str_to_int(string s){ 
    int num=0; int po=1; 
    rloop(s.le()-1,0){ 
        num+=po*(s[i]-'0');
        po*=10; 
